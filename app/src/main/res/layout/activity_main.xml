<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/padding"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/editName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/name"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/viewName"
        android:layout_width="91dp"
        android:layout_height="28dp"
        android:layout_marginTop="32dp"
        android:text="@string/viewname"
        android:textAlignment="center"
        android:textSize="24sp"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/done_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="92dp"
        android:text="@string/done"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/a1"
        style="@style/Box"
        android:layout_marginStart="60dp"
        android:layout_marginTop="176dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/a2"
        style="@style/BBb"
        app:layout_constraintStart_toEndOf="@+id/a1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/a3"
        style="@style/BBb"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:layout_marginTop="176dp"
        app:layout_constraintStart_toEndOf="@+id/a2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/a4"
        style="@style/BBb"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:layout_marginTop="176dp"
        app:layout_constraintStart_toEndOf="@+id/a3"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/a5"
        style="@style/BBb"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:layout_marginTop="176dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/a4"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/b1"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:layout_marginStart="60dp"
        android:background="#FFEB3B"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/a1" />

    <TextView
        android:id="@+id/b2"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        app:layout_constraintStart_toEndOf="@+id/b1"
        app:layout_constraintTop_toBottomOf="@+id/a2" />

    <TextView
        android:id="@+id/b3"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        app:layout_constraintStart_toEndOf="@+id/b2"
        app:layout_constraintTop_toBottomOf="@+id/a3" />

    <TextView
        android:id="@+id/b4"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        app:layout_constraintStart_toEndOf="@+id/b3"
        app:layout_constraintTop_toBottomOf="@+id/a4" />

    <TextView
        android:id="@+id/b5"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/b4"
        app:layout_constraintTop_toBottomOf="@+id/a5" />

    <TextView
        android:id="@+id/c1"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:layout_marginStart="60dp"
        android:background="#FFEB3B"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b1" />

    <TextView
        android:id="@+id/c2"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        app:layout_constraintStart_toEndOf="@+id/c1"
        app:layout_constraintTop_toBottomOf="@+id/b2" />

    <TextView
        android:id="@+id/c3"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        app:layout_constraintStart_toEndOf="@+id/c2"
        app:layout_constraintTop_toBottomOf="@+id/b3" />

    <TextView
        android:id="@+id/c4"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        app:layout_constraintStart_toEndOf="@+id/c3"
        app:layout_constraintTop_toBottomOf="@+id/b4" />

    <TextView
        android:id="@+id/c5"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/c4"
        app:layout_constraintTop_toBottomOf="@+id/b5" />

    <TextView
        android:id="@+id/d1"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:layout_marginStart="60dp"
        android:background="#FFEB3B"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/c1" />

    <TextView
        android:id="@+id/d2"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        app:layout_constraintStart_toEndOf="@+id/d1"
        app:layout_constraintTop_toBottomOf="@+id/c2" />

    <TextView
        android:id="@+id/d3"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        app:layout_constraintStart_toEndOf="@+id/d2"
        app:layout_constraintTop_toBottomOf="@+id/c3" />

    <TextView
        android:id="@+id/d4"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        app:layout_constraintStart_toEndOf="@+id/d3"
        app:layout_constraintTop_toBottomOf="@+id/c4" />

    <TextView
        android:id="@+id/d5"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        android:hapticFeedbackEnabled="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/d4"
        app:layout_constraintTop_toBottomOf="@+id/c5" />

    <TextView
        android:id="@+id/e1"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:layout_marginStart="60dp"
        android:background="#FFEB3B"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/d1" />

    <TextView
        android:id="@+id/e2"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/e1"
        app:layout_constraintTop_toBottomOf="@+id/d2"
        app:layout_constraintVertical_bias="0.004" />

    <TextView
        android:id="@+id/e3"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/e2"
        app:layout_constraintTop_toBottomOf="@+id/d3"
        app:layout_constraintVertical_bias="0.004" />

    <TextView
        android:id="@+id/e4"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/e3"
        app:layout_constraintTop_toBottomOf="@+id/d4" />

    <TextView
        android:id="@+id/e5"
        android:layout_width="@dimen/Box_dim"
        android:layout_height="@dimen/Box_dim"
        android:background="#FFEB3B"
        android:padding="@dimen/padding"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/e4"
        app:layout_constraintTop_toBottomOf="@+id/d5" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/retry"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.547"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/e3"
        app:layout_constraintVertical_bias="0.336" />

    <TextView
        android:id="@+id/game_over"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="GAME OVER!"
        android:textAlignment="center"
        android:textSize="24sp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.842"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.437" />
</androidx.constraintlayout.widget.ConstraintLayout>